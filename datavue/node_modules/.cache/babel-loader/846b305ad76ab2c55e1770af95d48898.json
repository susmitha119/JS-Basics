{"ast":null,"code":"import Vue from \"vue\";\nimport axios from \"axios\";\nimport VueAxios from \"vue-axios\";\nVue.use(VueAxios, axios);\nexport default {\n  name: \"NewFile\",\n\n  data() {\n    return {\n      arr: [],\n      id: '',\n      name: '',\n      departement: '',\n      isEdit: false,\n      row: '',\n      dialog: false,\n      add: true\n    };\n  },\n\n  mounted() {\n    Vue.axios.get('http://127.0.0.1:33797/read/').then(resp => this.arr = resp.data);\n  },\n\n  methods: {\n    postData() {\n      Vue.axios.post(\"http://127.0.0.1:33797/create/\", {\n        id: this.id,\n        name: this.name,\n        departement: this.departement\n      }).then(response => {\n        console.log(response.data);\n        this.id = '', this.name = \"\", this.departement = \"\";\n      });\n    },\n\n    async deleteData(id) {\n      await Vue.axios.delete(`http://127.0.0.1:33797/delete/${id}`);\n      Vue.axios.get('http://127.0.0.1:33797/read/').then(resp => this.arr = resp.data);\n    },\n\n    editdata(id) {\n      this.dialog = true; // this.isEdit=true\n\n      this.id = this.arr[id].id;\n      this.name = this.arr[id].name;\n      this.departement = this.arr[id].departement;\n      this.row = id; // this.isEdit=true\n    },\n\n    editInfo() {\n      // let editData = {}\n      // editData.id = this.id\n      // console.log(editData);\n      Vue.axios.patch('http://127.0.0.1:33797/update/').then(response => {\n        console.log(response.data);\n        this.getData();\n      });\n      this.isEdit = false;\n      this.name = \"\";\n      this.departement = \"\";\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAiGA;AACA;AACA;AACAA;AAEA;EACAC,eADA;;EAEAC;IACA;MACAC,OADA;MAEAC,MAFA;MAGAH,QAHA;MAIAI,eAJA;MAKAC,aALA;MAMAC,OANA;MAOAC,aAPA;MAQAC;IARA;EAYA,CAfA;;EAgBAC;IACAV;EACA,CAlBA;;EAmBAW,SACA;IACAC;MACAZ,iDACA;QACAI,WADA;QAEAH,eAFA;QAGAI;MAHA,CADA,EAKAQ,IALA,CAKAC;QACAC;QACA,cACA,cADA,EAEA,qBAFA;MAGA,CAVA;IAWA,CAbA;;IAeA,qBACA;MAEA;MACAf;IAIA,CAvBA;;IA0