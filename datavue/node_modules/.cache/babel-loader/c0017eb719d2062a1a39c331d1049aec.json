{"ast":null,"code":"import Vue from \"vue\";\nimport axios from \"axios\";\nimport VueAxios from \"vue-axios\";\nVue.use(VueAxios, axios);\nexport default {\n  name: \"NewFile\",\n\n  data() {\n    return {\n      arr: undefined\n    };\n  },\n\n  mounted() {\n    Vue.axios.get('http://127.0.0.1:3333/read').then(resp => this.arr = resp.data);\n  },\n\n  methods: {\n    submitData() {\n      Vue.axios.post(\"http://127.0.0.1:3333/create/\", {\n        name: this.name,\n        departement: this.departement\n      }).then(response => {\n        console.log(response.data);\n        this.name = \"\";\n        this.departement = \"\";\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAuBA;AACA;AACA;AACAA;AAEA;EACAC,eADA;;EAEAC;IACA;MAAAC;IAAA;EAEA,CALA;;EAMAC;IACAJ;EACA,CARA;;EASAK;IACAC;MACAN,UACAO,IADA,CACA,+BADA,EAEA;QACAN,eADA;QAEAO;MAFA,CAFA,EAKAC,IALA,CAKAC;QACAC;QACA;QACA;MACA,CATA;IAYA;;EAdA;AATA","names":["Vue","name","data","arr","mounted","methods","submitData","post","departement","then","response","console"],"sourceRoot":"src/components","sources":["newFile.vue"],"sourcesContent":["<template>\n    <v-simple-table>\n        <thead>\n            <tr>\n            <th>\n                id\n            </th>\n            <th>name</th>\n            <th>departement</th>\n        </tr>\n        </thead>\n        <tbody>\n            <tr v-for=\"(data,i) in arr\" :key=\"i\">\n               \n                <td>{{data.id}}</td>\n                <td>{{data.name}}</td>\n                <td>{{data.departement}}</td>\n            \n       </tr>\n        </tbody>\n    </v-simple-table>\n</template>\n<script>\n    import  Vue from \"vue\"\n    import axios from\"axios\"\n    import VueAxios from \"vue-axios\"\n    Vue.use(VueAxios,axios)\n\n    export default{\n        name:\"NewFile\",\n        data(){\n            return {arr:undefined}\n\n        },\n        mounted(){\n            Vue.axios.get('http://127.0.0.1:3333/read').then((resp)=>this.arr=resp.data